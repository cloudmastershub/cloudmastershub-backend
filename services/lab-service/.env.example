PORT=3003
NODE_ENV=development
LOG_LEVEL=debug

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# AWS Configuration (for lab provisioning)
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1
AWS_LAB_ROLE_ARN=arn:aws:iam::123456789012:role/CloudMastersHubLabRole

# Lab Resource Limits
MAX_LAB_DURATION=3600 # seconds
MAX_CONCURRENT_LABS=100
LAB_CLEANUP_DELAY=300 # seconds after session end